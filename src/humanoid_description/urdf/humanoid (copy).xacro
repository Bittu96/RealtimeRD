  <link name='foot'>
    <inertial>
      <mass value="10.0"/>
      <origin xyz="0.1 0.0 0.0" rpy=" 0 0 0"/>
      <inertia
          ixx="0.5" ixy="0" ixz="0"
          iyy="1.0" iyz="0"
          izz="0.1"
      />
    </inertial>
    <collision name='collision'>
      <origin xyz="0.1 0.0 0.0" rpy=" 0 0 0"/>
      <geometry>
        <box size=".4 .2 .1"/>
      </geometry>
    </collision>
    <visual name='foot_visual'>
      <origin xyz="0.1 0.0 0.0" rpy=" 0 0 0"/>
      <geometry>
        <box size=".4 .2 .1"/>
      </geometry>
    </visual>
  </link>


  <!--lllllllllllllllllllllllllllllllllllllllllllllllllllllllllllll           joints 1         lllllllllllllllllllllllllllllllllllllllll/-->
  <joint name="waist_joint" type="revolute">
    <axis xyz="1 0 0"/>
    <limit effort="1000.0" lower="-0.5" upper="0.5" velocity="0.0"/>
    <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
    <parent link="hip"/>
    <child link="waist"/>
  </joint>

  <joint name="hip_joint" type="revolute">
    <axis xyz="0 1 0"/>
    <limit effort="1000.0" lower="-0.5" upper="0.5" velocity="0.0"/>
    <origin rpy="0 0 0" xyz="0.0 0.0 1.0"/>
    <parent link="thigh"/>
    <child link="hip"/>
  </joint>

  <joint name="knee_joint" type="revolute">
    <axis xyz="0 1 0"/>
    <limit effort="1000.0" lower="-1.57" upper="0.0" velocity="0.0"/>
    <origin rpy="0 0 0" xyz="0.0 0.0 1.0"/>
    <parent link="shank"/>
    <child link="thigh"/>
  </joint>

  <joint name="ankle_joint" type="revolute">
    <axis xyz="0 1 0"/>
    <limit effort="1000.0" lower="-0.4" upper="0.4" velocity="0.0"/>
    <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
    <parent link="ankle"/>
    <child link="shank"/>
  </joint>

  <joint name="anklel_joint" type="revolute">
    <axis xyz="1 0 0"/>
    <limit effort="1000.0" lower="-0.4" upper="0.4" velocity="0.0"/>
    <origin rpy="0 0 0" xyz="0.0 0.0 0.1"/>
    <parent link="foot"/>
    <child link="ankle"/>
  </joint>

  <!--joint name="ankle_jo" type="fixed">
