<?xml version='1.0'?>

<robot name="humanoid" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <xacro:include filename="$(find humanoid_description)/urdf/humanoid.gazebo" />
  <xacro:include filename="$(find humanoid_description)/urdf/materials.xacro" />
  <xacro:include filename="$(find humanoid_description)/urdf/macros.xacro" />

  <link name='waist'>
    <inertial>
      <mass value="10.0"/>
      <origin xyz="0.0 0.0 0.5" rpy=" 0 0 0"/>
      <inertia
          ixx="0.5" ixy="0" ixz="0"
          iyy="1.0" iyz="0"
          izz="0.1"
      />
    </inertial>
    <collision name='collision'>
      <origin xyz="0.0 0.0 0.5" rpy=" 0 0 0"/>
      <geometry>
        <box size=".2 .5 .8"/>
      </geometry>
    </collision>
    <visual name='foot_visual'>
      <origin xyz="0.0 0.0 0.5" rpy=" 0 0 0"/>
      <geometry>
        <box size=".2 .4 .8"/>
      </geometry>
    </visual>
  </link>

  <link name="hip">
    <!--origin xyz="0.1 -0.13 0.1" rpy="0 1.5707 1.5707"/-->
    <collision name="collision">
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.05" length="0.1"/>
      </geometry>
    </collision>
    <visual name="hip_visual">
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.05" length="0.1"/>
      </geometry>
    </visual>
    <inertial>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <mass value="1"/>
      <cylinder_inertia m="1" r="0.05" h="0.1"/>
      <inertia
        ixx="1.0" ixy="0.0" ixz="0.0"
        iyy="1.0" iyz="0.0"
        izz="1.0"/>
    </inertial>
  </link>

  <link name='thigh'>
    <inertial>
      <mass value="10.0"/>
      <origin xyz="0.0 0.0 0.51" rpy=" 0 0 0"/>
      <inertia
          ixx="0.5" ixy="0" ixz="0"
          iyy="1.0" iyz="0"
          izz="0.1"
      />
    </inertial>
    <collision name='collision'>
      <origin xyz="0.0 0.0 0.51" rpy=" 0 0 0"/>
      <geometry>
        <box size=".1 .1 0.8"/>
      </geometry>
    </collision>
    <visual name='thigh_visual'>
      <origin xyz="0.0 0.0 0.51" rpy=" 0 0 0"/>
      <geometry>
        <box size=".1 .1 0.8"/>
      </geometry>
    </visual>
  </link>


